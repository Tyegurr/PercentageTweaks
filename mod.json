{
	"geode": "4.10.0",
	"gd": {
		"win": "2.2074",
		"android": "2.2074",
		"mac": "2.2074",
		"ios": "2.2074"
	},
	"id": "tyegurr.percentagetweaks",
	"name": "PercentageTweaks",
	"version": "v1.0.0",
	"developer": "Tyegurr",
	"description": "Tweak your percentages!",
	"tags": ["customization"],
	"dependencies": {
		"geode.node-ids": ">=1.21.0"
	},
	"settings": {
		"percentage-precision": {
			"name": "Percentage Precision",
			"type": "int",
			"min": 2,
			"max": 100,
			"default": 5,
			"control": {
				"slider": true
			}
		},
		"scientific-notation": {
			"name": "Scientific Notation",
			"type": "bool",
			"default": false
		},
		"percentage-label-scale": {
			"name": "Percentage Label Scale",
			"type": "float",
			"default": 0.5,
			"min": 0.25,
			"max": 1.75,
			"control": {
				"slider": true
			}
		}
	}
}
